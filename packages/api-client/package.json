{
  "name": "@canvastencil/api-client",
  "version": "1.0.0",
  "description": "API client service layer for CanvaStencil platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cms": {
      "import": "./dist/cms/index.mjs",
      "require": "./dist/cms/index.js",
      "types": "./dist/cms/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@canvastencil/types": "file:../types",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0"
  },
  "keywords": [
    "canvastencil",
    "api-client",
    "axios",
    "typescript"
  ],
  "author": "CanvaStack Team",
  "license": "MIT"
}
