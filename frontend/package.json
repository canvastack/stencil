    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "test:visual": "playwright test src/__tests__/e2e/visual-regression.spec.ts",
    "chromatic": "chromatic --playwright",
    "chromatic:review": "chromatic --playwright --exit-zero-on-changes",
    "serve:dist-root": "node scripts/serve-dist-root.cjs",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev:frontend": "vite",
    "dev:backend": "cd ../backend && npm run serve",
    "dev:all": "concurrently --names \"FRONTEND,BACKEND\" --prefix \"[{name}]\" \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:both": "npm run dev:all"
